# Environment Configuration
# Copy this file to .env and update with your actual values

# Node Environment (development, production, test)
NODE_ENV=development

# Server Port
PORT=5000

# Database URL
DATABASE_URL=postgresql://user:password@host:port/database

# CORS Configuration
# IMPORTANT: In production, you MUST set this to your specific domain
# Example: CORS_ORIGIN=https://yourdomain.com
# If not set in production, CORS will be disabled for security
# For development, wildcard '*' is used by default
CORS_ORIGIN=*

# Cache Configuration (in seconds)
CACHE_TTL=300

# Session Configuration
# Generate a secure random string for production
SESSION_SECRET=your-secure-session-secret-here
